{
  "name": "oracle",
  "version": "0.0.1",
  "description": "AI-powered repository Q&A",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "oracle": "./dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/lib/index.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "dev": "tsx src/cli/index.ts",
    "cli": "node dist/cli/index.js",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "repository",
    "qa",
    "rag",
    "ai"
  ],
  "author": "Oracle Contributors",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@anthropic-ai/tokenizer": "^0.0.4",
    "@commander-js/extra-typings": "^14.0.0",
    "@huggingface/transformers": "^3.8.1",
    "@inquirer/prompts": "^8.2.1",
    "@orama/orama": "^3.1.18",
    "@orama/plugin-data-persistence": "^3.1.18",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.6.2",
    "cli-highlight": "^2.1.11",
    "cli-progress": "^3.12.0",
    "cohere-ai": "^7.20.0",
    "commander": "^14.0.3",
    "cosmiconfig": "^9.0.0",
    "fast-json-stable-stringify": "^2.1.0",
    "ignore": "^6.0.2",
    "is-ci": "^4.1.0",
    "isbinaryfile": "^5.0.7",
    "js-tiktoken": "^1.0.15",
    "lru-cache": "^11.2.6",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-persist": "^4.0.4",
    "ollama": "^0.6.3",
    "onnxruntime-node": "^1.24.1",
    "openai": "^4.104.0",
    "ora": "^9.3.0",
    "simple-git": "^3.31.1",
    "terminal-link": "^5.0.0",
    "tree-sitter-javascript": "^0.21.4",
    "tree-sitter-python": "^0.21.0",
    "tree-sitter-typescript": "^0.21.2",
    "tree-sitter-wasms": "^0.1.13",
    "usearch": "^2.21.4",
    "web-tree-sitter": "^0.23.2"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cli-progress": "^3.11.6",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^25.2.3",
    "@types/node-persist": "^3.1.8",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{ts,js,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
